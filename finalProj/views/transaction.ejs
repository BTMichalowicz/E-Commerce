<%include partials/header.ejs %>
<div class="table-wrapper">
    <% if (Buys.length > 0 ) { %>
      <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">ID</th>

                <th scope="col">Item Type</th>
                <th scope="col"> Price</th>
                <th scope="col">Quantity</th>
                <th scope="col">ItemName</th>
                <th scope="col">Seller ID</th>
            </tr>
        </thead>

      <tbody>
          <% Item.forEach((item, index) => { %>
          <tr>
              <th scope="row"><%= item.ItemId %></th>


              <td><%= item.ItemType %></td>
                <td>$<%= item.Price%></td>
              <td><%= item.Quantity %></td>
              <td><%= item.ItemName %></td>

          </tr>
          <% }) %>
      </tbody>
  </table>
  <% } else { %>
  <p class="text-center">No Items found. </p>
  <% } %>
</div>
</div>
<br><br><br>
     <footer class="container">
      <p class="float-right"><a href="/list_Items">Back to top</a></p>
      <p>© 2019 Database Designers Pro, Inc. <!-- · <a href="#">Privacy</a> · <a href="#">Terms</a> --></p>
    </footer>



</body>
</html>
</div>
