<%include partials/header.ejs %>
<div class="table-wrapper">
    <% if (Seller.length > 0) {%>
    <table class="table table-hovered">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Seller ID</th>
                <th scope="col">Seller Name</th>
                
            </tr>
        </thead>
        <tbody>
            <% Seller.forEach((seller, index) => { %>
            <tr>
                <th scope="row"><%= seller.SellerId %></th>
                <td><%= seller.SellerName %></td>
                <td>
                   
                    <a href="/delete/<%= seller.SellerId %>" class="btn btn-sm btn-danger">Delete</a>
                </td>
               
               
            </tr>
            <% }) %>
        </tbody>
    </table>
    <% } else { %>
    <p class="text-center">No Sellers Exist!!</p>
    <% } %>
</div>
</div>
<br><br><br>
       <footer class="container">
        <p class="float-right"><a href="/list_Sellers">Back to top</a></p>
        <p>© 2019 Database Designers Pro, Inc. <!-- · <a href="#">Privacy</a> · <a href="#">Terms</a> --></p>
      </footer>



  </body>
  </html>